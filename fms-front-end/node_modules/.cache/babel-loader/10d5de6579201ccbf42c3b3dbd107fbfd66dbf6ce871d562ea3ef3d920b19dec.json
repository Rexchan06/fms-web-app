{"ast":null,"code":"import \"core-js/modules/esnext.iterator.constructor.js\";\nimport \"core-js/modules/esnext.iterator.for-each.js\";\nimport axios from \"axios\";\nexport default {\n  name: 'App',\n  data() {\n    return {\n      items: [],\n      newItem: {\n        name: \"\",\n        description: \"\"\n      },\n      selectedFile: null\n    };\n  },\n  methods: {\n    handleFileChange(event) {\n      this.selectedFile = event.target.files[0];\n    },\n    handleUpdateFile(event, item) {\n      item.newFile = event.target.files[0];\n    },\n    getFileDownloadURL(item) {\n      return `http://localhost:3002/files/${item.id}`;\n    },\n    fetchItems() {\n      axios.get(\"http://localhost:3002/items\").then(response => {\n        this.items = response.data;\n      });\n    },\n    addItem() {\n      if (!this.selectedFile) {\n        alert(\"Please select a file\");\n        return;\n      }\n      const formData = new formData();\n      formData.append(\"name\", this.newItem.name);\n      formData.append(\"description\", this.newItem.description);\n      formData.append(\"file\", this.selectedFile);\n      axios.post(\"http://localhost:3002/items\", formData, {\n        header: {\n          \"Content-Type\": \"multipart/form-data\"\n        }\n      }).then(() => {\n        this.fetchItems();\n        this.newItem.name = \"\";\n        this.newItem.description = \"\";\n        this.selectedFile = null;\n        if (this.$refs.fileInput) {\n          this.$refs.fileInput.value = \"\";\n        }\n      });\n    },\n    updateItem(item) {\n      const formData = new formData();\n      formData.append(\"name\", item.name);\n      formData.append(\"description\", item.description);\n      if (item.newFile) {\n        formData.append(\"file\", item.newFile);\n      }\n      axios.put(`http://localhost:3002/items/${item.id}`, formData, {\n        header: {\n          \"Content-Type\": \"multipart/form-data\"\n        }\n      }).then(() => {\n        this.fetchItems();\n        item.newFile = null;\n        // Find the file input for this item and reset it\n        const fileInputs = document.querySelectorAll('input[type=\"file\"]');\n        fileInputs.forEach(input => {\n          input.value = \"\";\n        });\n      }).catch(error => {\n        console.error(\"Error updating item:\", error);\n      });\n    },\n    deleteItem(id) {\n      axios.delete(`http://localhost:3002/items/${id}`).then(() => {\n        this.fetchItems();\n      });\n    }\n  },\n  created() {\n    this.fetchItems();\n  }\n};","map":{"version":3,"names":["axios","name","data","items","newItem","description","selectedFile","methods","handleFileChange","event","target","files","handleUpdateFile","item","newFile","getFileDownloadURL","id","fetchItems","get","then","response","addItem","alert","formData","append","post","header","$refs","fileInput","value","updateItem","put","fileInputs","document","querySelectorAll","forEach","input","catch","error","console","deleteItem","delete","created"],"sources":["D:\\Rex\\HELP University\\Y2S1\\File Management System Project\\fms-app\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <h1>My File Management System</h1>\n    \n    <div class=\"form-container\">\n      <input v-model=\"newItem.name\" placeholder=\"name\">\n      <input v-model=\"newItem.description\" placeholder=\"description\">\n      <input\n        type=\"file\"\n        @change=\"handleFileChange\"\n        ref=\"fileInput\"\n        class=\"file-input\"\n      >\n      <button @click=\"addItem\">Upload File</button>\n    </div>\n\n    <div class=\"items-container\">\n      <ul>\n        <li v-for=\"item in items\" :key=\"item.id\" class=\"item\">\n          <div class=\"item-info\">\n            <input v-model=\"item.name\">\n            <input v-model=\"item.description\">\n            <a :href=\"getFileDownloadURL(item)\" class=\"download-link\">Download File</a>\n            <span class=\"file-name\">{{ item.originalName }}</span>\n          </div>\n          <div class=\"item-actions\">\n            <input\n              type=\"file\"\n              @change=\"(e) => handleUpdateFile(e, item)\"\n              ref=\"`updateFile_${item.id}`\"\n              class=\"file-input\"\n            >\n            <button @click=\"updateItem(item)\">Update</button>\n            <button @click=\"deleteItem(item.id)\" class=\"delete-btn\">Delete</button>\n          </div>\n        </li>\n      </ul>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: 'App',\n  data() {\n    return {\n      items: [],\n      newItem: {\n        name: \"\",\n        description: \"\",\n      },\n      selectedFile: null,\n    };\n  },\n\n  methods: {\n    handleFileChange(event) {\n      this.selectedFile = event.target.files[0];\n    },\n\n    handleUpdateFile(event, item) {\n      item.newFile = event.target.files[0];\n    },\n\n    getFileDownloadURL(item) {\n      return `http://localhost:3002/files/${item.id}`;\n    },\n\n    fetchItems() {\n      axios.get(\"http://localhost:3002/items\")\n        .then(response => {\n          this.items = response.data;\n        });\n    },\n\n    addItem() {\n      if (!this.selectedFile) {\n        alert(\"Please select a file\");\n        return;\n      }\n\n      const formData = new formData();\n      formData.append(\"name\", this.newItem.name);\n      formData.append(\"description\", this.newItem.description);\n      formData.append(\"file\", this.selectedFile);\n\n      axios.post(\"http://localhost:3002/items\", formData, {\n        header: {\n          \"Content-Type\": \"multipart/form-data\"\n        }\n      }).then(() => {\n          this.fetchItems();\n          this.newItem.name = \"\";\n          this.newItem.description = \"\";\n          this.selectedFile = null;\n          if (this.$refs.fileInput) {\n            this.$refs.fileInput.value = \"\";\n          }\n        });\n    },\n\n    updateItem(item) {\n      const formData = new formData();\n      formData.append(\"name\", item.name);\n      formData.append(\"description\", item.description);\n      if (item.newFile) {\n        formData.append(\"file\", item.newFile);\n      }\n\n      axios.put(`http://localhost:3002/items/${item.id}`, formData, {\n        header: {\n          \"Content-Type\": \"multipart/form-data\"\n        }\n      }).then(() => {\n          this.fetchItems();\n          item.newFile = null;\n        // Find the file input for this item and reset it\n        const fileInputs = document.querySelectorAll('input[type=\"file\"]');\n        fileInputs.forEach(input => {\n          input.value = \"\";\n        });\n      })\n      .catch(error => {\n        console.error(\"Error updating item:\", error);\n        });\n    },\n\n    deleteItem(id) {\n      axios.delete(`http://localhost:3002/items/${id}`)\n        .then(() => {\n          this.fetchItems();\n        });\n    },\n  },\n\n  created() {\n    this.fetchItems();\n  },\n};\n</script>\n\n<style scoped>\n#app {\n  max-width: 800px;\n  margin: 0 auto;\n  padding: 20px;\n}\n\nh1 {\n  text-align: center;\n}\n\n.form-container {\n  margin: 20px 0;\n  display: flex;\n  gap: 10px;\n  justify-content: center;\n}\n\n.items-container {\n  margin-top: 20px;\n}\n\n.item {\n  display: flex;\n  gap: 10px;\n  margin: 10px 0;\n  padding: 10px;\n  border: 1px solid #ddd;\n  border-radius: 4px;\n  align-items: center;\n}\n\nul {\n  list-style: none;\n  padding: 0;\n}\n\ninput {\n  padding: 8px;\n  border: 1px solid #ddd;\n  border-radius: 4px;\n}\n\nbutton {\n  padding: 8px 16px;\n  background-color: #4CAF50;\n  color: white;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n}\n\nbutton:hover {\n  background-color: #45a049;\n}\n</style>"],"mappings":";;AA0CA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE;QACPH,IAAI,EAAE,EAAE;QACRI,WAAW,EAAE;MACf,CAAC;MACDC,YAAY