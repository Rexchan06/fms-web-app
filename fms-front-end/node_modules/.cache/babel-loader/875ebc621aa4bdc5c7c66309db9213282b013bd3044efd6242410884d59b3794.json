{"ast":null,"code":"import axios from \"axios\";\nexport default {\n  name: 'App',\n  data() {\n    return {\n      items: [],\n      loading: false,\n      error: null,\n      newItem: {\n        name: \"\",\n        description: \"\"\n      }\n    };\n  },\n  methods: {\n    async fetchItems() {\n      this.loading = true;\n      try {\n        console.log('Fetching items...');\n        const response = await axios.get(\"http://localhost:3001/items\");\n        console.log('Response:', response.data);\n        this.items = response.data;\n      } catch (error) {\n        console.error(\"Error fetching items:\", error);\n        this.error = error.message;\n      } finally {\n        this.loading = false;\n      }\n    },\n    async addItem() {\n      try {\n        await axios.post(\"http://localhost:3001/items\", this.newItem);\n        this.fetchItems();\n        this.newItem.name = \"\";\n        this.newItem.description = \"\";\n      } catch (error) {\n        console.error(\"Error adding item:\", error);\n      }\n    }\n  },\n  async created() {\n    console.log('Component created');\n    await this.fetchItems();\n  }\n};","map":{"version":3,"names":["axios","name","data","items","loading","error","newItem","description","methods","fetchItems","console","log","response","get","message","addItem","post","created"],"sources":["D:\\Rex\\HELP University\\Y2S1\\File Management System Project\\fms-app\\src\\App.vue"],"sourcesContent":["<template>\n  <div>\n    <h1>My File Management System</h1>\n    <!-- Debug info -->\n    <div>\n      <p>Number of items: {{ items.length }}</p>\n      <p>Loading status: {{ loading ? 'Loading...' : 'Done' }}</p>\n    </div>\n    \n    <!-- Input form -->\n    <div>\n      <input v-model=\"newItem.name\" placeholder=\"name\">\n      <input v-model=\"newItem.description\" placeholder=\"description\">\n      <button @click=\"addItem\">Create File</button>\n    </div>\n\n    <!-- Items list with debug info -->\n    <div>\n      <h2>Items:</h2>\n      <pre>{{ JSON.stringify(items, null, 2) }}</pre>\n      \n      <ul v-if=\"items.length > 0\">\n        <li v-for=\"item in items\" :key=\"item.id\">\n          {{ item.name }} - {{ item.description }}\n        </li>\n      </ul>\n      <p v-else>No items found</p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: 'App',\n  data() {\n    return {\n      items: [],\n      loading: false,\n      error: null,\n      newItem: {\n        name: \"\",\n        description: \"\",\n      },\n    };\n  },\n\n  methods: {\n    async fetchItems() {\n      this.loading = true;\n      try {\n        console.log('Fetching items...');\n        const response = await axios.get(\"http://localhost:3001/items\");\n        console.log('Response:', response.data);\n        this.items = response.data;\n      } catch (error) {\n        console.error(\"Error fetching items:\", error);\n        this.error = error.message;\n      } finally {\n        this.loading = false;\n      }\n    },\n\n    async addItem() {\n      try {\n        await axios.post(\"http://localhost:3001/items\", this.newItem);\n        this.fetchItems();\n        this.newItem.name = \"\";\n        this.newItem.description = \"\";\n      } catch (error) {\n        console.error(\"Error adding item:\", error);\n      }\n    },\n  },\n\n  async created() {\n    console.log('Component created');\n    await this.fetchItems();\n  },\n};\n</script>\n\n<style scoped>\n.input-container {\n  margin: 20px 0;\n}\n\n.input-container input {\n  margin: 0 10px;\n  padding: 5px;\n}\n\n.items-list {\n  margin-top: 20px;\n}\n\n.item {\n  margin: 10px 0;\n  list-style: none;\n}\n\n.item input {\n  margin: 0 10px;\n  padding: 5px;\n}\n\nbutton {\n  margin: 0 5px;\n  padding: 5px 10px;\n  cursor: pointer;\n}\n\nul {\n  padding: 0;\n}\n</style>"],"mappings":"AAgCA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MA